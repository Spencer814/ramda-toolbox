{
  "name": "ramda-toolbox",
  "version": "1.0.0",
  "private": true,
  "author": "Andre Newman",
  "license": "MIT",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir build --extensions \".ts\"",
    "start": "babel-node -x \".js\" index.js",
    "test": "jest -c jest.config.js",
    "watch": "jest -c jest.config.js --watch",
    "watchAll": "jest -c jest.config.js --watchAll",
    "test-cov": "jest -c jest.config.js && open artifacts/index.html",
    "test-report": "jest -c jest.config.js && open artifacts/report.html",
    "docs": "rm -rf docs; typedoc --exclude '**/*.spec.ts' --excludeExternals src; open docs/index.html",
    "lint": "eslint -c .eslintrc.js --ext .ts src",
    "lint-fix": "eslint -c .eslintrc.js --ext .ts src --fix"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/"
  },
  "engines": {
    "node": ">=12"
  },
  "peerDependencies": {
    "bluebird": "^3.7.2",
    "ramda": "^0.27.1"
  },
  "dependencies": {
    "bluebird": "^3.7.2",
    "ramda": "^0.27.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/eslint-parser": "^7.11.3",
    "@babel/eslint-plugin": "^7.11.3",
    "@babel/node": "^7.10.5",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.11.0",
    "@babel/plugin-transform-modules-commonjs": "^7.10.4",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-typescript": "^7.10.4",
    "@babel/types": "^7.11.0",
    "@types/bluebird": "^3.5.32",
    "@types/eslint": "^7.2.0",
    "@types/jest": "^26.0.9",
    "@types/ramda": "^0.27.14",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "babel-node": "0.0.1-security",
    "eslint": "^7.6.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^9.0.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-order-alphabetical": "1.0.1",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-ramda": "^2.5.1",
    "eslint-plugin-sort-destructure-keys": "^1.3.5",
    "eslint-plugin-sort-keys-fix": "^1.1.1",
    "jest": "^26.3.0",
    "jest-environment-node": "^26.3.0",
    "jest-html-reporters": "^2.0.2",
    "jest-junit": "^11.1.0",
    "jest-runner-eslint": "^0.10.0",
    "jest-runner-tsc": "^1.6.0",
    "jest-watch-continue": "^1.1.4",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-suspend": "^1.1.2",
    "jest-watch-toggle-config": "^2.0.1",
    "ts-jest": "^26.2.0",
    "typedoc": "^0.18.0",
    "typescript": "^3.9.7"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Spencer814/ramda-toolbox.git"
  },
  "bugs": {
    "url": "https://github.com/Spencer814/ramda-toolbox/issues"
  },
  "homepage": "https://github.com/Spencer814/ramda-toolbox#readme",
  "description": ""
}
